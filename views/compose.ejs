<%- include('partials/header'); %>
  <h1>Compose</h1>
  <form  action="/compose" method="post">

  <div class="mb-3 form-group">
    <label for="title" class="form-label">Title:</label>
    <input type="text" class="form-control" id="exampleInputEmail1" name="postTitle" aria-describedby="emailHelp">
  </div>

  <div class="mb-3 form-group">
    <label for="postBody" class="form-label">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="80"></textarea>
  </div>

  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); %>
